{
  "version": 2,
  "builds": [
    { "src": "api/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/scrape", "dest": "/api/scrape.js" },
    { "src": "/api/analyze", "dest": "/api/analyze.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}